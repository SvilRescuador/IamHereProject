<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Bulunanlar">

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="319dp"
        android:layout_height="51dp"
        android:layout_marginTop="8dp"
        android:contentDescription="asd"
        android:keepScreenOn="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName12"></Spinner>

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="319dp"
        android:layout_height="51dp"
        android:layout_marginTop="8dp"
        android:contentDescription="asd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner6"></Spinner>

    <Spinner
        android:id="@+id/spinner7"
        android:layout_width="319dp"
        android:layout_height="51dp"
        android:layout_marginTop="8dp"
        android:contentDescription="asd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner5"></Spinner>

    <Spinner
        android:id="@+id/spinner6"
        android:layout_width="319dp"
        android:layout_height="51dp"
        android:layout_marginTop="12dp"
        android:contentDescription="asd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner"></Spinner>

    <EditText
        android:id="@+id/editTextTextPersonName10"
        android:layout_width="328dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="Name-Surname"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName12"
        android:layout_width="328dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="Phone Number"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName10" />

    <Button
        android:id="@+id/button3"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#C61313"
        android:onClick="back"
        android:text="Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <Button
        android:id="@+id/button7"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#C61313"
        android:onClick="makeNotice"
        android:text="Make Inform"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName14" />

    <EditText
        android:id="@+id/editTextTextPersonName14"
        android:layout_width="290dp"
        android:layout_height="90dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:gravity="bottom|left"
        android:hint="Open Adress"
        android:inputType="textPersonName"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner7" />
</androidx.constraintlayout.widget.ConstraintLayout>

